{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style":"off",
        "keyword-spacing":"error",
        "space-infix-ops":"error",
        "import/prefer-default-export":"off",
        "max-len":"off",
        "import/extensions":"off",
        "import/no-unresolved":"off",
        "no-plusplus":"off",
        "no-await-in-loop":"off"
    }
}
